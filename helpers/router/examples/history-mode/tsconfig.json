{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": ["es2017", "dom"],
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build",
    "sourceMap": true,
    "strict": true,
    "target": "es6",
    "types": ["preact"]
  },
  "include": [
    "./src/**/*.ts"
  ]
}
