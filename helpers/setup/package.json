{
  "name": "meiosis-setup",
  "version": "5.1.0",
  "private": true,
  "description": "Meiosis Helpers - setup",
  "main": "meiosis-setup.js",
  "scripts": {
    "start": "rollup -c && tsc && cp -r src/* README.md api.md dist/ && cat package.json|grep -v private>dist/package.json",
    "watch": "rollup -c -w",
    "test": "npm start && tape 'tests/**/*.js' | tap-spec",
    "docs": "documentation build src/index.js -f md -o api.md",
    "server": "static-server -p ${PORT:-9000}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxdonut/meiosis.git"
  },
  "keywords": [
    "meiosis",
    "reactive",
    "state",
    "state management"
  ],
  "author": "foxdonut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxdonut/meiosis/issues"
  },
  "homepage": "https://github.com/foxdonut/meiosis#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "buble": "^0.20.0",
    "documentation": "^13.0.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "flyd": "^0.2.8",
    "immer": "^7.0.9",
    "mergerino": "0.4.0",
    "mithril": "^2.0.4",
    "prettier": "^2.1.2",
    "ramda": "^0.27.1",
    "rollup": "^2.27.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "static-server": "^2.2.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "typescript": "^4.0.3"
  }
}
