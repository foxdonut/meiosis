{
  "name": "docs",
  "version": "4.0.0",
  "private": true,
  "description": "meiosis tutorial",
  "main": "index.js",
  "scripts": {
    "prebuild": "mkdir -p build dist && node add-flems.js lessons build",
    "build": "harp compile build dist && (cd images && ./generate_all_diagrams.sh)",
    "postbuild": "cp css/* images/*.svg images/*.png images/*.gif dist",
    "onchange": "onchange -i app.css app.html 'common/*' 'css/*' 'lessons/*' 'code/**' -- npm run build",
    "start": "http-server . -p ${PORT:-9000}",
    "reload": "reload -w code,dist -e html,css,js,jsx -p ${PORT:-9000}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxdonut/meiosis.git"
  },
  "keywords": [
    "example",
    "meiosis",
    "reactive",
    "react",
    "virtual dom"
  ],
  "author": "foxdonut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxdonut/meiosis/issues"
  },
  "homepage": "https://github.com/foxdonut/meiosis#readme",
  "devDependencies": {
    "harp": "^0.32.0",
    "http-server": "^0.12.3",
    "onchange": "^7.0.2",
    "reload": "^3.1.0",
    "static-server": "^2.2.1"
  },
  "dependencies": {}
}
